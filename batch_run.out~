#!/bin/bash
#SBATCH --job-name=ar-l
#SBATCH --output=ar-l-char.out
#SBATCH --partition=holyseasgpu
#SBATCH --qos=short
#SBATCH --gres=gpu:1
#SBATCH --nodes=1
#SBATCH --tasks-per-node=1
#SBATCH -t 3-23:00
#SBATCH --mem=24000
#SBATCH --mail-type=END
#SBATCH --mail-user=yhk255@nyu.edu

sleep 60
th main.lua -data_dir data/ar -EOS '' -gpuid 0 -cudnn 1 -use_words 0 -use_chars 1
